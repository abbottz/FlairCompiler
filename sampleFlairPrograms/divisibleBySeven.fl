program divisibleBySeven(number : integer);
   function split(num : integer) : integer
      var left : integer;
	      right : integer;
		  diff : integer;
	  begin
	     left := num / 10;
		 right := (num - (left * 10)) * 2;
		 diff := left - right;
		 if diff = 0 then
		    return 1
	     else if diff = -7 then
		    return 1
		 else if diff = 7 then
		    return 1
		 else if diff < 14 then
		    return 0
		 else
		    return(split(diff))
	  end;
begin
   print(split(number))
end.